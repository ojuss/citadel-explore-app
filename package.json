{
  "name": "explore-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "pnpm:start": "pnpm expo start",
    "pnpm:android": "pnpm expo start --android",
    "pnpm:ios": "pnpm expo start --ios",
    "pnpm:web": "pnpm expo start --web",
    "clean": "rimraf node_modules .pnpm-store",
    "clean:metro": "rimraf node_modules/.cache/metro",
    "setup": "pnpm install && pnpm clean:metro",
    "reset": "pnpm clean && pnpm install && pnpm clean:metro"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.0.0",
    "expo": "^53.0.19",
    "expo-blur": "~14.1.5",
    "expo-linear-gradient": "~14.1.5",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "5.4.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "^19.0.0",
    "@types/react-native": "^0.73.0",
    "metro": "^0.82.0",
    "metro-minify-terser": "^0.77.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "metro-resolver": "^0.77.0",
    "metro-runtime": "^0.77.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
